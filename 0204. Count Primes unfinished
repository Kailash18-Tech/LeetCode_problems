                      204. Count Primes

#include <cmath>

int countPrimes(int n) {
    if (n <= 1) return 0;
    
    int countprime = 0;
    
    // Helper function to check if a number is prime
    bool isprime(int x) {
        if (x <= 1) return false;
        for (int j = 2; j * j <= x; j++) {  // Check up to the square root of x
            if (x % j == 0) return false;    // If divisible, it's not prime
        }
        return true;
    }
    
    for (int i = 2; i < n; i++) {  // Check all numbers from 2 to n-1
        if (isprime(i)) {
            countprime++;
        }
    }
    
    return countprime;
}






int countPrimes(int n) 
{   int countprime=0;
    if(n<=1)
    return 0;
    int isprime(int x)
    {
        for(int j=2;j<x;j++)
        {
            if(x%j==0)
            countprime++;
        }
        return countprime;
    }
    for(int i=2;i<=n;i++)
    {
      int isprime(int i)
    }
}
